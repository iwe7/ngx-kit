{
  "name": "@ngx-kit/schematcs-src",
  "version": "1.0.0-alpha.6",
  "description": "Schematics for ngx-kit",
  "keywords": [
    "angular",
    "ngx-kit",
    "schematics"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:tsc && npm run build:release",
    "build:clean": "rimraf ./dist",
    "build:tsc": "tsc -p tsconfig.json",
    "build:release": "node release",
    "publish": "npm publish ./dist/package --access=public",
    "release": "npm run build && npm run publish",
    "build-and-copy": "npm run build && ngx-kit copy"
  },
  "author": "Sasha Novik",
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/schematics": "0.0.30",
    "@ngtools/json-schema": "^1.0.9",
    "@types/glob": "^5.0.29",
    "@types/istanbul": "^0.4.29",
    "@types/jasmine": "^2.5.47",
    "@types/minimist": "^1.2.0",
    "@types/node": "^6.0.36",
    "@types/semver": "^5.3.30",
    "@types/source-map": "^0.5.0",
    "@types/webpack": "^3.0.2",
    "@types/webpack-sources": "^0.1.3",
    "conventional-changelog": "^1.1.0",
    "glob": "^7.0.3",
    "istanbul": "^0.4.5",
    "jasmine": "^2.6.0",
    "jasmine-spec-reporter": "^3.2.0",
    "loader-utils": "^1.1.0",
    "minimist": "^1.2.0",
    "npm": "^5.2.0",
    "npm-run": "^4.1.2",
    "npm-run-all": "^4.0.0",
    "rxjs": "^5.0.1",
    "semver": "^5.3.0",
    "source-map": "^0.5.6",
    "tar": "^3.1.5",
    "temp": "^0.8.3",
    "ts-node": "^2.0.0",
    "tslint": "^5.5.0",
    "typescript": "~2.4.0",
    "v8-profiler": "^5.7.0",
    "webpack-sources": "^1.0.1"
  },
  "devDependencies": {
    "fs-extra": "^4.0.2",
    "rimraf": "^2.6.2"
  }
}

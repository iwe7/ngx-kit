{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsNgxKitDemo",
  "title": "Ngx-kit Options Schema",
  "type": "object",
  "properties": {
    "path": {
      "type": "string",
      "default": "app/package/demo"
    },
    "appRoot": {
      "type": "string"
    },
    "sourceDir": {
      "type": "string",
      "default": "src",
      "alias": "sd"
    },
    "name": {
      "type": "string"
    }
  },
  "required": [
    "name"
  ]
}
